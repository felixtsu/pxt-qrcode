{
    "name": "qrcode",
    "version": "0.0.5",
    "description": "A QR code generator for MakeCode",
    "license": "MIT",
    "dependencies": {
        "core": "*",
        "screen": "*"
    },
    "files": [
        "bitbuffer.ts",
        "errorcorrectlevel.ts",
        "maskpattern.ts",
        "mode.ts",
        "polynomial.ts",
        "qr8bitbyte.ts",
        "qralphanum.ts",
        "qrcode.ts",
        "qrdata.ts",
        "qrnumber.ts",
        "qrmath.ts",
        "qrutil.ts",
        "rsblock.ts",
        "api.ts",
        "README.md"
    ],
    "testFiles": [
        "test.ts"
    ],
    "testDependencies": {
        "game": "*"
    },
    "public": true,
    "supportedTargets": [
        "arcade"
    ],
    "preferredEditor": "tsprj",
    "extension": {
        "namespace": "images",
        "label": "QR Code",
        "localUrl": "http://127.0.0.1:3000/index.html"
    }
}
